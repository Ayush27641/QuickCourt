# Supabase Postgres (direct connection) - recommended for Prisma `migrate` / `db push`
# Format example:
# DATABASE_URL="postgresql://postgres:<PASSWORD>@db.<PROJECT_REF>.supabase.co:5432/postgres?sslmode=require"
DATABASE_URL=""

# Supabase Postgres (session pooler / PgBouncer) - recommended for runtime pooling
# Format example:
# POOLED_DATABASE_URL="postgresql://postgres.<PROJECT_REF>:<PASSWORD>@aws-1-<REGION>.pooler.supabase.com:5432/postgres?pgbouncer=true&sslmode=require"
POOLED_DATABASE_URL=""

# Base64 secret key (to match Java JwtService which expects base64)
JWT_SECRET_BASE64=""

PORT=3001
